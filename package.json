{
    "name": "dart-code",
    "displayName": "Dart Code",
    "description": "Dart support for Visual Studio Code.",
    "version": "0.1.0",
    "publisher": "DanTup",
    "engines": {
        "vscode": "^1.0.0"
    },
    "license": "SEE LICENSE IN LICENSE FILE",
    "bugs": {
        "url": "https://github.com/DanTup/Dart-Code/issues"
    },
    "homepage": "https://github.com/DanTup/Dart-Code/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/DanTup/Dart-Code.git"
    },
    "categories": [
        "Languages",
        "Linters"
    ],
    "keywords": [
        "dart",
        "dartlang",
        "pub",
        "pubspec"
    ],
    "icon": "media/icon.png",
    "activationEvents": [
        "onLanguage:dart",
        "onLanguage:pubspec",
        "workspaceContains:pubspec.yaml"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "dart",
                "extensions": [
                    ".dart"
                ],
                "aliases": [
                    "Dart"
                ]
            },
            {
                "id": "pubspec",
                "filenames": [
                    "pubspec.yaml"
                ],
                "aliases": [
                    "Pubspec"
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}